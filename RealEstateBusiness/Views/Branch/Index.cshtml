@model List<RealEstateBusiness.Models.Branch>
@using RealEstateBusiness.Models

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <h1>Branches</h1>
        @foreach(Branch branch in Model)
        {
            @Html.ActionLink(branch.BranchNo, "StaffinBranch", "Home", new { @branchNo = branch.BranchNo }, null);
            <br>
        }
    </div>
</body>
</html>
