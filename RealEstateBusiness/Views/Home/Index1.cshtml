@model List<RealEstateBusiness.Models.Owner>
@using RealEstateBusiness.Models

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index1</title>
</head>
<body>
    <div> 
        <h1>Owners Details</h1>
        <table border="1">
            <tr>
                <td>Owner No</td>
                <td>First Name</td>
                <td>Last Name</td>
                <td>Address</td>
                <td>Telephone No</td>
            </tr>

            @foreach(Owner owner in Model)
            {
                <tr>
                    <td>@owner.OwnerNo</td>
                    <td>@owner.Fname</td>
                    <td>@owner.Lname</td>
                    <td>@owner.Address</td>
                    <td>@owner.TelNo</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
